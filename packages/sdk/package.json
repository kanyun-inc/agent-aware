    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.48.0",
    "@rslib/core": "^0.3.0",
    "@vitest/coverage-v8": "^2.1.0",
